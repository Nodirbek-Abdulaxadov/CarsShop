@using CarsShop.Data.Entities
@model Image

<h2>Create</h2>

<form asp-action="Create">
    <div class="form-horizontal">
        <div class="form-group">
            <label class="control-label">Id</label>
            <input type="number" asp-for="Id" id="idId" class="form-control" />
            <span asp-validation-for="Id" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label class="control-label">ColorId</label>
            <input type="number" asp-for="ColorId" id="idColorId" class="form-control" />
            <span asp-validation-for="ColorId" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label class="control-label">Url</label>
            <input type="text" asp-for="Url" id="idUrl" class="form-control" />
            <span asp-validation-for="Url" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Create" class="btn btn-primary" />
        </div>
    </div>
</form>

<div>
    <a asp-action="Index">Back to List</a>
</div>
